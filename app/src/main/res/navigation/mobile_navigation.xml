<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="ru.project.technomotors_1.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_car"
        android:name="ru.project.technomotors_1.ui.car.CarFragment"
        android:label="@string/title_car"
        tools:layout="@layout/fragment_car" />

    <fragment
        android:id="@+id/navigation_service"
        android:name="ru.project.technomotors_1.ui.service.ServiceFragment"
        android:label="@string/title_service"
        tools:layout="@layout/fragment_service" />
    <fragment
        android:id="@+id/maintenanceFormFragment"
        android:name="ru.project.technomotors_1.MaintenanceFormFragment"
        android:label="@string/title_maintenanceFormFragment"
        tools:layout="@layout/fragment_maintenance_form" />
    <fragment
        android:id="@+id/select_model_list"
        android:name="ru.project.technomotors_1.SelectModelFragment"
        android:label="Выбрать модель"
        tools:layout="@layout/fragment_select_model" />
    <fragment
        android:id="@+id/select_year_list"
        android:name="ru.project.technomotors_1.SelectYearFragment"
        android:label="Выбрать год покупки"
        tools:layout="@layout/fragment_select_year" />
    <fragment
        android:id="@+id/select_TO_list"
        android:name="ru.project.technomotors_1.SelectTOFragment"
        android:label="Вид ТО"
        tools:layout="@layout/fragment_select_t_o" />
    <fragment
        android:id="@+id/select_data"
        android:name="ru.project.technomotors_1.SelectDataFragment"
        android:label="Выбор даты и времени"
        tools:layout="@layout/fragment_select_data" />
    <fragment
        android:id="@+id/select_time"
        android:name="ru.project.technomotors_1.SelectTimeFragment"
        android:label="Выбрать время"
        tools:layout="@layout/fragment_select_time" />
    <fragment
        android:id="@+id/select_contact_client"
        android:name="ru.project.technomotors_1.ContactClientFragment"
        android:label="fragment_contact_client"
        tools:layout="@layout/fragment_contact_client" />
</navigation>